{
	"version": 2,
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		},
		{
			"src": "/static/projects/([^/]+)/thumb.png",
			"dest": "/static/projects/$1/thumb.png"
		},
		{
			"src": "/projects/([^/]+)/index.html",
			"dest": "/static/projects/$1/index.html"
		},
		{
			"src": "/projects/([^/]+)/(.*\\.(js|css|mp3|png|jpg|svg))",
			"dest": "/static/projects/$1/$2"
		},
		{
			"src": "/static/icons/(.*)",
			"dest": "/static/icons/$1"
		}
	],
	"rewrites": [
		{
			"source": "/static/:path*",
			"destination": "/static/:path*"
		},
		{
			"source": "/projects/:slug",
			"destination": "/projects/:slug"
		},
		{
			"source": "/projects/:slug/view",
			"destination": "/projects/:slug/view"
		}
	]
}
